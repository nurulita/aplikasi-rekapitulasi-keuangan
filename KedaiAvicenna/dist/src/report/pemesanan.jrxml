	<queryString>
		<![CDATA[SELECT
     pemesanan.`no_pemesanan` AS pemesanan_no_pemesanan,
     pemesanan.`tglpesan` AS pemesanan_tglpesan,
     pemesanan.`id_pemesanan` AS pemesanan_id_pemesanan,
     pemesanan.`id_tenant` AS pemesanan_id_tenant,
     pemesanan.`namatenant` AS pemesanan_namatenant,
     pemesanan.`id_menu` AS pemesanan_id_menu,
     pemesanan.`namamenu` AS pemesanan_namamenu,
     pemesanan.`hargasatuan` AS pemesanan_hargasatuan,
     pemesanan.`jumlahpesan` AS pemesanan_jumlahpesan,
     pemesanan.`total` AS pemesanan_total,
     pemesanan.`no_meja` AS pemesanan_no_meja
FROM
     `pemesanan` pemesanan
WHERE
     pemesanan.`tglpesan` BETWEEN $P{tgl1} AND $P{tgl2} OR
     pemesanan.`tglpesan` BETWEEN $P{tgl1} AND $P{tgl2}]]>
