	<queryString>
		<![CDATA[SELECT
     rekapitulasi_mingguan.`id_rekapmingguan` AS rekapitulasi_mingguan_id_rekapmingguan,
     rekapitulasi_mingguan.`tanggalmulai` AS rekapitulasi_mingguan_tanggalmulai,
     rekapitulasi_mingguan.`tanggalakhir` AS rekapitulasi_mingguan_tanggalakhir,
     rekapitulasi_mingguan.`id_tenant` AS rekapitulasi_mingguan_id_tenant,
     rekapitulasi_mingguan.`namatenant` AS rekapitulasi_mingguan_namatenant,
     rekapitulasi_mingguan.`pendapatankotor` AS rekapitulasi_mingguan_pendapatankotor,
     rekapitulasi_mingguan.`potongan` AS rekapitulasi_mingguan_potongan,
     rekapitulasi_mingguan.`pendapatankedai` AS rekapitulasi_mingguan_pendapatankedai,
     rekapitulasi_mingguan.`pendapatanbersih` AS rekapitulasi_mingguan_pendapatanbersih
FROM
     `rekapitulasi_mingguan` rekapitulasi_mingguan
WHERE
     rekapitulasi_mingguan.`tanggalmulai` BETWEEN $P{tgl1}and$P{tgl2}
  OR rekapitulasi_mingguan.`tanggalakhir` BETWEEN $P{tgl1}and$P{tgl2}]]>
	</queryString>
