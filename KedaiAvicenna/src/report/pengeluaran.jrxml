	<parameter name="cari" class="java.lang.String"/>
	<parameter name="defaultValue" class="java.lang.Integer">
		<defaultValueExpression><![CDATA[new Integer("1")]]></defaultValueExpression>
	</parameter>
	<queryString>
		<![CDATA[SELECT
     pengeluaran.`id_pengeluaran` AS pengeluaran_id_pengeluaran,
     pengeluaran.`tanggal` AS pengeluaran_tanggal,
     pengeluaran.`keterangan` AS pengeluaran_keterangan,
     pengeluaran.`jumlah` AS pengeluaran_jumlah
FROM
     `pengeluaran` pengeluaran
WHERE
     pengeluaran.`tanggal` BETWEEN $P{tgl1} AND $P{tgl2} OR
     pengeluaran.`id_pengeluaran` like '%$P!{cari}%' OR
     pengeluaran.`keterangan` like '%$P!{cari}%' OR
     pengeluaran.`jumlah` like '%$P!{cari}%']]>
	</queryString>
